{
  "name": "webrtc-connection-tester",
  "version": "2.0.0-alpha-14",
  "type": "module",
  "description": "This is an NPM package to test your webrtc connection before joining call or meetings",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs"
  },
  "keywords": [
    "react",
    "webrtc",
    "testrtc",
    "npm webrtc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/prashil-vaishnani/webrtc-connection-tester"
  },
  "bugs": {
    "url": "https://github.com/prashil-vaishnani/webrtc-connection-tester/issues"
  },
  "peerDependencies": {
    "react": ">= 17.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.13",
    "@types/react": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2"
  },
  "author": "prashil vaishnani ",
  "contributors": [
    {
      "name": "Alexander Maisey",
      "url": "https://github.com/alexandermaisey"
    }
  ],
  "license": "MIT"
}
